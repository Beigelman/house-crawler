{
  "compilerOptions": {
    "strict": true
  },
  "tasks": {
    "run": "deno run --allow-net --allow-write --allow-env --allow-read src/search_properties.ts",
    "validate": "deno run --allow-net --allow-write --allow-env --allow-read src/validate_properties.ts",
    "lint": "deno lint",
    "fmt": "deno fmt"
  },
  "imports": {
    "cheerio": "npm:cheerio@^1.1.2",
    "@std/async": "jsr:@std/async@^1",
    "@std/dotenv": "jsr:@std/dotenv@^0.225",
    "@supabase/supabase-js": "npm:@supabase/supabase-js@^2",
    "resend": "npm:resend@^4"
  },
  "fmt": {
    "options": {
      "useTabs": false,
      "lineWidth": 80,
      "indentWidth": 2,
      "singleQuote": true,
      "proseWrap": "preserve"
    }
  }
}
